{
  "name": "Web Annotator App",
  "description": "Tool to be used with the Web Annotator Extension to allow saving of the annotations to disk",
  "version": "0.0.3",
  "manifest_version": 2,
  "key": "",
  "minimum_chrome_version": "31",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": {
    "16": "icons/icon-16.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    },
    "storage"
  ],
  "file_handlers": {
    "text": {
      "types": [
        "text/*"
      ]
    }
  }
}